{
  "name": "escolalms/mailerlite",
  "description": "Escola LMS MailerLite integration.",
  "type": "package",
  "require": {
    "php": ">=7.4",
    "escolalms/auth": "^0",
    "escolalms/core": "^1",
    "escolalms/settings": "^0",
    "guzzlehttp/guzzle": "^7",
    "laravel/framework": ">=8.0",
    "mailerlite/mailerlite-api-v2-php-sdk": "^0.3.2",
    "php-http/guzzle7-adapter": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "orchestra/testbench": ">=5.0",
    "escolalms/cart": "^0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Maciej Rymarz",
      "email": "maciej.rymarz@escolasoft.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "EscolaLms\\MailerLite\\": "src",
      "EscolaLms\\MailerLite\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "EscolaLms\\MailerLite\\EscolaLmsMailerLiteServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
